{
  "name": "instgram",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/app.css --config tailwind.config.cjs",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/app.css --watch --config tailwind.config.cjs",
    "start": "run-p watch:css dev"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "firebase": "^8.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "gh-pages": "^6.2.0",
    "globals": "^15.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.9"
  }
}
